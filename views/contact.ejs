</head>

<%- include('partials/header') %> <!-- Include header and navbar from partials -->
<body>
    <!-- Section-Contact -->
    <section class="Contact-section py-5 bg-dark text-white">
        <div class="container text-center">
            <h2 class="pb-2 border-bottom text-body-emphasis">Contact Me</h2>
            <p class="lead mb-4">Feel free to reach out through any of the platforms below:</p>
            <div class="d-grid gap-2 col-md-4 mx-auto">
                <a href="https://www.linkedin.com/in/your-linkedin-profile" target="_blank">
                    <button type="button" class="btn btn-warning btn-lg">LinkedIn</button>
                </a>
                <a href="https://github.com/xyzsimon34/" target="_blank">
                    <button type="button" class="btn btn-outline-secondary btn-lg">GitHub</button>
                </a>
                <a href="mailto:your-email@example.com">
                    <button type="button" class="btn btn-info btn-lg">Email Me</button>
                </a>
            </div>
        </div>
    </section>
</body>
<%- include('partials/footer') %> <!-- Include footer from partials -->
</html>